{
  "name": "vite-enterprise-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --open",
    "test:unit": "jest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test:e2e": "cypress run-ct",
    "test:int": "cypress open-ct",
    "lint": "npx pnpm gen:eslintrc && eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "npx pnpm gen:eslintrc && eslint \"**/*.{vue,ts,js}\" --fix",
    "build": "cross-env NODE_ENV=production vite-ssg build",
    "serve": "vite preview",
    "serve:https": "serve dist",
    "gen:eslintrc": "node scripts/generate-eslintrc.js && prettier .eslintrc.js -w"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
